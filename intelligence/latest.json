          "edition": {
            "pattern": "edition = \"2021\"",
            "description": "Use Rust 2021 edition for latest features"
          }
        },
        "deprecatedPatterns": {
          "rust-analyzer.checkOnSave-boolean": {
            "pattern": "\"rust-analyzer.checkOnSave\": true",
            "replacement": "\"rust-analyzer.checkOnSave\": { \"enable\": true, \"command\": \"clippy\" }",
            "severity": "error",
            "deprecatedSince": "2023-06-01",
            "reason": "Boolean syntax no longer supported, use object configuration"
          },
          "edition-2018": {
            "pattern": "edition = \"2018\"",
            "replacement": "edition = \"2021\"",
            "severity": "warning",
            "deprecatedSince": "2021-10-21",
            "reason": "Rust 2021 edition provides better defaults and new features"
          }
        }
      },
      "python": {
